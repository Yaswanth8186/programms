#python 

x = int(input("Enter number of rows: "))
for i in range(1,x + 1):
  for j in range(1,i + 1):
    print(i,end ="")
  print()

2.palindrome number

#python

num = int(input("Enter a number : "))
temp = num
rev = 0
while(num != 0):
  rem = num % 10
  rev = rev * 10 + rem
  num = num // 10
if(temp == rev):
  print("Palindrome  number")
else:
  print("Not palindrome number")

3.Armstrong number

#python

num = int(input("Enter a number : "))
temp = num
sum = 0
while(num != 0):
  rem = num % 10
  sum = sum + rem * rem * rem
  num = num // 10
if(sum == temp):
  print("Armstrong  number")
else:
  print("Not Armstrong number")
  
4.To find area of triangle, rectangle and square

#python

side_of_square = int(input("Enter the length of side of square : "))
print("The area of square is : ", side_of_square * side_of_square)
length_of_rectangle = int(input("Enter the length of rectangle : "))
breadth_of_rectangle = int(input("Enter the breadth of rectangle : "))
print("The area of rectangle is : ", length_of_rectangle * breadth_of_rectangle)
base_of_triangle = int(input("Enter the length of base of triangle : "))
height_of_triangle = int(input("E"))

5.To swap three numbers in ascending order
#python

first = int(input("Enter first number : "))
second = int(input("Enter second number : "))
third = int(input("Enter third number : "))
if(first > second and first > third):
    print("hi")
    temp = third
    third = first
    if(temp > second):
        first = second
        second = temp
    else:
        first = temp
elif(second > first and second > third):
    print("hello")
    temp = third
    third = second
    if(temp > first):
        second = temp
    else:
        second = first
        first = temp
else:
    if(first > second):
        temp = first
        first = second
        second = temp
print(first,second,third)
